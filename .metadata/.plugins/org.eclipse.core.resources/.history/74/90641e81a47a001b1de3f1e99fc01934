package com.inter.codepract.array;

public class RearrangeArrayWithAlternateHigLow {

	static void alter(int arr[]) {
		int i=0;
		int j=1;
		
		while(j<arr.length) {
			if(arr[i]>arr[j]) {
				int temp = arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
			}
			if(j+1<arr.length && arr[j]<arr[j+1]) {
				
			}
		}
	}
}
