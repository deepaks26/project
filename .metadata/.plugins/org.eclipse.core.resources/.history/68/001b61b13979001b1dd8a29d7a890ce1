package com.demo.inter.linkedlist.fromtechidelite;

public class SplitLinkedListTwoHalves {

	static class Node{
		int data;
		Node next;
		Node(int data){
			this.data = data;
		}
	}
	
	void split(Node head) {
		Node slow = head;
		Node fast = head;
		while(slow != null && fastPtr.next != null) {
			slow = slow.next;
			fast = fast.next;
		}
	}
}
