package com.inter.design.visitor2;

public class VisitorImpl implements Visitor {

	int totalPrice;
	
	@Override
	public int visit(Book book) {
		totalPrice +=totalPrice+ book.N*book.price;
		return totalPrice;
	}

	@Override
	public int visit(CD cd) {
		totalPrice +=totalPrice+ cd.N*cd.price;
		return totalPrice;
	}

}
